{
    "name": "goldenmaster",
    "private": true,
    "version": "1.0.0",
    "description": "goldenmaster",
    "license": "MIT",
    "files": [
        "dist/"
    ],
    "scripts": {
        "build-win": "node -e \"console.log(new Date().getTime())\" > timestamp.tmp && set /p NAMESPACE=<timestamp.tmp&& stencil build --log && node build.js",
        "build": "NAMESPACE=$(node -e \"console.log(new Date().getTime())\") && NAMESPACE=$NAMESPACE stencil build --log && NAMESPACE=$NAMESPACE node build.js",
        "start": "stencil build --dev --watch --serve --log",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watch"
    },
    "dependencies": {
        "@ionic/core": "4.4.2",
        "@prismadelabs/prismaid": "^1.2.8",
        "@prismadelabs/prismaidui": "^1.0.2-beta.9",
        "rxjs": "^6.5.3",
        "cheerio": "^1.0.0-rc.3"
    },
    "devDependencies": {
        "@stencil/core": "0.18.1",
        "rollup-plugin-node-builtins": "^2.1.2",
        "workbox-build": "3.4.1"
    },
    "prettier": {
        "tabWidth": 4,
        "semi": false,
        "singleQuote": false,
        "trailingComma": "all",
        "bracketSpacing": true,
        "arrowParens": "always",
        "endOfLine": "lf",
        "proseWrap": "never",
        "printWidth": 150
    }
}
