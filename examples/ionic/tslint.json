{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
    "semicolon": [
        true,
        "never"
    ],
    "no-console": [
        false
    ],
    "no-duplicate-variable": true,
    "no-unused-variable": [
        false
    ],
    "max-line-length": [
        false,
        150
    ]
  },
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ]
}
